{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "inputs": [
        "*",
        "!.github/workflows/**",
        "!e2e/**",
        "!.vscode/**",
        "!.husky/**"
      ]
    },
    "type-check": {},
    "format": {},
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "turbo": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "test": {
      "cache": false
    }
  }
}
